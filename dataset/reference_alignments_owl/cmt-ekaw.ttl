@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://cmt#Author> a owl:Class ;
    owl:equivalentClass <http://ekaw#Paper_Author> .

<http://cmt#Conference> a owl:Class ;
    owl:equivalentClass <http://ekaw#Conference> .

<http://cmt#ConferenceMember> a owl:Class ;
    owl:equivalentClass <http://ekaw#Conference_Participant> .

<http://cmt#Document> a owl:Class ;
    owl:equivalentClass <http://ekaw#Document> .

<http://cmt#Paper> a owl:Class ;
    owl:equivalentClass <http://ekaw#Paper> .

<http://cmt#PaperFullVersion> a owl:Class ;
    owl:equivalentClass <http://ekaw#Regular_Paper> .

<http://cmt#Person> a owl:Class ;
    owl:equivalentClass <http://ekaw#Person> .

<http://cmt#Review> a owl:Class ;
    owl:equivalentClass <http://ekaw#Review> .

<http://cmt#assignedTo> a owl:ObjectProperty ;
    owl:equivalentProperty <http://ekaw#hasReviewer> .

<http://cmt#hasBeenAssigned> a owl:ObjectProperty ;
    owl:equivalentProperty <http://ekaw#reviewerOfPaper> .

<http://cmt#writtenBy> a owl:FunctionalProperty ;
    owl:equivalentProperty <http://ekaw#reviewWrittenBy> .

